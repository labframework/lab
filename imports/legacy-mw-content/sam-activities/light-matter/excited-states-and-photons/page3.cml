<paragraph start="8" end="13">
<Alignment>1</Alignment>
<SpaceBelow>5.0</SpaceBelow>
<content start="8" end="9">
<component><class>org.concord.modeler.PageSlider</class>
<modelclass>org.concord.mw2d.models.MolecularModel</modelclass>
<model>0</model>
<value>10.0</value>
<minimum>0.0</minimum>
<maximum>30.0</maximum>
<nstep>30</nstep>
<title>Speed of Atom A (m/s)</title>
<width>190</width>
<height>250</height>
<orientation>1</orientation>
<opaque>false</opaque>
<disabled_at_run>true</disabled_at_run>
<tick>true</tick>
<border>Line</border>
<labeltable>{value=&quot;10.0&quot;, label=&quot;1000&quot;}{value=&quot;20.0&quot;, label=&quot;2000&quot;}{value=&quot;30.0&quot;, label=&quot;3000&quot;}{value=&quot;5.0&quot;, label=&quot;500&quot;}{value=&quot;15.0&quot;, label=&quot;1500&quot;}{value=&quot;25.0&quot;, label=&quot;2500&quot;}{value=&quot;0.0&quot;, label=&quot;0&quot;}</labeltable>
<script>set %v1 &quot;%val*100&quot;;
set atom[0].vx %v1;
</script>
<change>Execute MW script</change>
</component></content>

